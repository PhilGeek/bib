@article{Gert:2012es,
	Abstract = {In The Red and the Real, Jonathan Cohen defends a relationalist view of color: the view that colors are constituted by relations between objects, perceivers, and circumstances. Cohen\textquoteright{}s defense of relationalism is often ingenious, but it also commits him to some extremely counterintuitive---{}one might say \textquotedblleft{}crazy\textquotedblright---{}claims. The present paper argues that the phenomena that are captured by Cohen\textquoteright{}s ingenious defense of his interesting view can be captured equally well by a more \textquotedblleft{}boring\textquotedblright view. Such a view distinguishes between colors and the ways that those colors appear to various viewers, takes colors to be relatively vague, and claims that other species with color vision simply see other colors. Since the boring view stays closer to common sense on many points, it is to be preferred},
	Author = {Gert, Joshua},
	Date-Added = {2014-08-20 14:24:05 +0000},
	Date-Modified = {2014-08-20 14:24:42 +0000},
	Journal = {Croatian Journal of Philosophy},
